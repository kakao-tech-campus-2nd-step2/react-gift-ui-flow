    "@storybook/node-logger": "8.1.11",
    "@storybook/types": "8.1.11",
    "@yarnpkg/fslib": "2.10.3",
    "@yarnpkg/libzip": "2.3.0",
    "chalk": "^4.1.0",
    "cross-spawn": "^7.0.3",
    "esbuild": "^0.18.0 || ^0.19.0 || ^0.20.0",
    "esbuild-register": "^3.5.0",
    "execa": "^5.0.0",
    "file-system-cache": "2.3.0",
